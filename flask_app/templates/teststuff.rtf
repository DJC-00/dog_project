[Unit]
Description=Gunicorn instance
After=network.target
[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/dog_test
Environment="PATH=/home/ubuntu/dog_test/venv/bin"
ExecStart=/home/ubuntu/dog_test/venv/bin/gunicorn --workers 3 --bind unix:flask_app.sock -m 007 wsgi:application
[Install]
WantedBy=multi-user.target

